drop view DEV_SUPPLY_CHAIN_VW.CORPORATE_CALENDAR_VW if exists;
drop table DEV_SUPPLY_CHAIN_VW.CORP_CAL if exists;

CREATE TABLE DEV_SUPPLY_CHAIN_VW.CORP_CAL
(

  START_DATE VARCHAR(10),
  END_DATE VARCHAR(10),
  CORPORATE_YEAR INT,
  CORPORATE_QUARTER INT,
  CORPORATE_MONTH INT,
  MOD_DT TIMESTAMP
)


CREATE VIEW DEV_SUPPLY_CHAIN_VW.CORPORATE_CALENDAR_VW
(
   START_DATE,
   END_DATE,
   CORPORATE_YEAR,
   CORPORATE_QUARTER,
   CORPORATE_MONTH,
   MOD_DT
)
AS
 SELECT
            START_DATE,
            END_DATE,
            CORPORATE_YEAR,
            CORPORATE_QUARTER,
            CORPORATE_MONTH,
            MOD_DT
   FROM   DEV_SUPPLY_CHAIN_VW.CORP_CAL;
