drop view DEV_SUPPLY_CHAIN_VW.PROJECTED_VISIBILITY_VW if exists;
drop table DEV_SUPPLY_CHAIN_VW.PROJECT_VISIBILITY if exists;

CREATE TABLE DEV_SUPPLY_CHAIN_VW.PROJECT_VISIBILITY
(
  HYBRID_VARIETY_ID     VARCHAR(250),
  ORG_L1_ID             VARCHAR(250),
  AS_OF_DATE            VARCHAR(250),
  PROJECTED_AVAILABILITY_UNITS       DECIMAL,
  UNIT_OF_MEASURE       VARCHAR(250),
  COUNTRY_ID            VARCHAR (250),
  MOD_DT                DATE
);

CREATE VIEW DEV_SUPPLY_CHAIN_VW.PROJECTED_VISIBILITY_VW
(
  HYBRID_VARIETY_ID,
  ORG_L1_ID,
  AS_OF_DATE,
  PROJECTED_AVAILABILITY_UNITS,
  UNIT_OF_MEASURE,
  COUNTRY_ID,
  MOD_DT
)
AS
   SELECT   HYBRID_VARIETY_ID,
            ORG_L1_ID,
            AS_OF_DATE,
            PROJECTED_AVAILABILITY_UNITS,
            UNIT_OF_MEASURE,
            COUNTRY_ID,
            MOD_DT
     FROM   DEV_SUPPLY_CHAIN_VW.PROJECT_VISIBILITY;


